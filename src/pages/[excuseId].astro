---
import Layout from '../layouts/Layout.astro';

export async function getStaticPaths() {
  const response = await fetch("https://script.google.com/macros/s/AKfycbwMOIu5a-_MJFM3sMt08nsNnSzBkYu4xOsAQSXb0Bg50Ahct2zrAmdHdxL6W6kqgA/exec");
  const data = await response.json();
  console.log("data", data)
  return data.map((d) => {
    return { params: d}
  });
}

const { excuseId } = Astro.params;
---

<Layout>
  <div>Good excuseId, {excuseId}!</div>
</Layout>
